	"name": "ratos-configurator",
	"private": true,
	"scripts": {
		"dev": "next dev | pino-pretty -c",
		"build": "next build",
		"start": "sleep 1 && curl -s -o /dev/null --retry 20 --retry-all-errors --retry-delay 1 --retry-max-time 60 \"http://localhost:3000/configure\" &> /dev/null &\nnext start",
		"lint": "next lint",
		"lint:ci": "next lint --format json --output-file eslint_report.json .",
		"test": "vitest",
		"test:config": "vitest configuration",
		"test:ci": "vitest  --reporter=verbose --reporter=junit",
		"test:ci:config": "vitest configuration  --reporter=verbose --reporter=junit",
		"typecheck": "tsc --noEmit",
